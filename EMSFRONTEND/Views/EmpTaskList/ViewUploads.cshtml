@{
}
@model IEnumerable<EMSFRONTEND.Models.UploadModel>

<h2>Employee Uploads</h2>

<table class="table">
    <thead>
        <tr>
            <th>UploadId</th>
            <th>TaskId</th>
            <th>UserId</th>
            <th>FilePath</th>
            <th>UploadedAt</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var upload in Model)
        {
            <tr>
                <td>@upload.UploadId</td>
                <td>@upload.TaskId</td>
                <td>@upload.UserId</td>
                <td>@upload.FilePath</td>
                <td>@upload.UploadedAt</td>

            </tr>
        }
    </tbody>
</table>
